# Google Gemini API Configuration
# Get this from: Google Cloud Console → APIs & Services → Credentials
# Create an API key and enable Generative Language API (Gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# Application Configuration
LOG_LEVEL=info
MAX_CONCURRENT_GENERATIONS=3
ASSET_OUTPUT_DIR=./generated-assets
MONTHLY_BUDGET_LIMIT=10.0

# Nano Banana (Gemini Image Generation) Settings
# Uses Gemini 2.5 Flash Image (preview) - latest image generation model
NANO_BANANA_MODEL=gemini-2.5-flash-image-preview
IMAGE_GENERATION_QUALITY=high
IMAGE_COST_PER_GENERATION=0.039