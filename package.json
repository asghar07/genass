{
  "name": "genass",
  "version": "1.0.0",
  "description": "AI-powered asset generation CLI with interactive mode. Intelligently scans codebases and generates missing visual assets using Google Gemini API and Nano Banana (Gemini 2.5 Flash Image). Features context memory, smart filenames, auto code injection, and cost tracking.",
  "main": "dist/index.js",
  "bin": {
    "genass": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\"",
    "publish:npm": "npm publish --access public"
  },
  "keywords": [
    "ai",
    "asset-generation",
    "assets",
    "automation",
    "cli",
    "cli-tool",
    "gemini",
    "google-gemini",
    "nano-banana",
    "codebase",
    "codebase-scanner",
    "developer-tools",
    "generative-ai",
    "generation",
    "icon-generator",
    "image-generation",
    "logo-generator",
    "interactive-cli",
    "typescript",
    "visual-assets",
    "smart-filenames",
    "cost-tracking",
    "context-memory"
  ],
  "author": "Asghar <asghar.07@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asghar07/genass.git"
  },
  "homepage": "https://github.com/asghar07/genass",
  "bugs": {
    "url": "https://github.com/asghar07/genass/issues"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "globby": "^14.0.0",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "mime-types": "^2.1.35",
    "ora": "^8.0.1",
    "sharp": "^0.33.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/jest": "^29.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
